{
  "name": "angular-playground",
  "description": "ES6 Angular built using webpack",
  "scripts": {
    "build": "webpack",
    "babel-node": "./node_modules/babel/bin/babel-node.js --stage 0 --ignore='target|frontend'",
    "start": "npm run build && ./node_modules/nodemon/bin/nodemon.js --ignore target --exec npm run babel-node -- backend/app.js"
  },
  "dependencies": {
    "angular": "^1.4.5",
    "express": "^4.13.3"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-loader": "^5.3.2",
    "eslint": "^1.4.1",
    "eslint-loader": "^1.0.0",
    "exports-loader": "^0.6.2",
    "html-webpack-plugin": "^1.6.1",
    "nodemon": "^1.5.0",
    "strict-loader": "^0.1.2",
    "webpack": "^1.12.1"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
